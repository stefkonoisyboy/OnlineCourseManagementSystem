@model OnlineCourseManagementSystem.Web.ViewModels.Files.FileByIdViewModel
@{
    this.ViewData["Title"] = "Info about file";
}

<style>
    .row-margin-05 {
        margin-top: 0.5em;
    }

    .row-margin-10 {
        margin-top: 1.0em;
    }

    .row-margin-20 {
        margin-top: 2.0em;
    }

    .row-margin-30 {
        margin-top: 3.0em;
    }

    svg.icon {
        padding: 20px;
        font-size: 4em;
        border-radius: 50%;
        border: 4px solid #234465;
        color: #234465;
        width: 115px;
        height: 115px;
    }

        svg.icon:hover {
            color: white;
            background-color: #234465;
        }
</style>

<head>
    <link rel="stylesheet" href="https://unpkg.com/swiper/swiper-bundle.css" />
    <link rel="stylesheet" href="https://unpkg.com/swiper/swiper-bundle.min.css" />

    <script src="https://unpkg.com/swiper/swiper-bundle.js"></script>
    <script src="https://unpkg.com/swiper/swiper-bundle.min.js"></script>
</head>

<body class="bg-light">
    <h4>
        <strong style="color: #234465;">@this.Model.LectureTitle - </strong>
        <strong class="text-info">@this.Model.LectureCourseName - </strong>
        <strong class="text-info">@this.Model.LectureCourseStartDate.ToString("MMMM yyyy")</strong>
    </h4>

    <div>
        <iframe src='https://view.officeapps.live.com/op/embed.aspx?src=@(this.Model.RemoteUrl)' width='80%' height='565px' frameborder='0'> </iframe>
    </div>

    <h3 style="color: #234465;">Resources from the lecture</h3>

    <div class="row">
        @foreach (var file in this.Model.Files.Where(f => f.Extension == ".mp4"))
        {
            <div class="col-md-4">
                <a asp-action="VideoById" asp-route-id="@file.Id" asp-route-courseId="@file.LectureCourseId">
                    <div class="shadow-lg p-3 mb-5 bg-white rounded text-center">
                        <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-camera-video-fill icon" viewBox="0 0 16 16">
                            <path fill-rule="evenodd" d="M0 5a2 2 0 0 1 2-2h7.5a2 2 0 0 1 1.983 1.738l3.11-1.382A1 1 0 0 1 16 4.269v7.462a1 1 0 0 1-1.406.913l-3.111-1.382A2 2 0 0 1 9.5 13H2a2 2 0 0 1-2-2V5z" />
                        </svg>
                        <p style="color: #337ab7;">(Video - @file.CreatedOn.ToString("dd MMMM yyyy") - @(file.UserFirstName + ' ' + file.UserLastName))</p>
                        <p style="color: #234465;">@file.LectureTitle</p>
                    </div>
                </a>
            </div>
        }

        @foreach (var file in this.Model.Files.Where(f => f.Extension == ".docx"))
        {
            <div class="col-md-4">
                <a asp-action="ById" asp-route-id="@file.Id" asp-route-lectureId="@file.LectureId">
                    <div class="shadow-lg p-3 mb-5 bg-white rounded text-center">
                        <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-file-earmark-word icon" viewBox="0 0 16 16">
                            <path d="M5.485 6.879a.5.5 0 1 0-.97.242l1.5 6a.5.5 0 0 0 .967.01L8 9.402l1.018 3.73a.5.5 0 0 0 .967-.01l1.5-6a.5.5 0 0 0-.97-.242l-1.036 4.144-.997-3.655a.5.5 0 0 0-.964 0l-.997 3.655L5.485 6.88z" />
                            <path d="M14 14V4.5L9.5 0H4a2 2 0 0 0-2 2v12a2 2 0 0 0 2 2h8a2 2 0 0 0 2-2zM9.5 3A1.5 1.5 0 0 0 11 4.5h2V14a1 1 0 0 1-1 1H4a1 1 0 0 1-1-1V2a1 1 0 0 1 1-1h5.5v2z" />
                        </svg>
                        <p style="color: #337ab7;">(Document - @file.CreatedOn.ToString("dd MMMM yyyy") - @(file.UserFirstName + ' ' + file.UserLastName))</p>
                        <p style="color: #234465;">@file.LectureTitle</p>
                    </div>
                </a>
            </div>
        }

        @foreach (var file in this.Model.Files.Where(f => f.Extension == ".pptx"))
        {
           <div class="col-md-4">
               <a asp-action="ById" asp-route-id="@file.Id" asp-route-lectureId="@file.LectureId">
                   <div class="shadow-lg p-3 mb-5 bg-white rounded text-center">
                       <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-file-earmark-ppt icon" viewBox="0 0 16 16">
                           <path d="M7 5.5a1 1 0 0 0-1 1V13a.5.5 0 0 0 1 0v-2h1.188a2.75 2.75 0 0 0 0-5.5H7zM8.188 10H7V6.5h1.188a1.75 1.75 0 1 1 0 3.5z" />
                           <path d="M14 4.5V14a2 2 0 0 1-2 2H4a2 2 0 0 1-2-2V2a2 2 0 0 1 2-2h5.5L14 4.5zm-3 0A1.5 1.5 0 0 1 9.5 3V1H4a1 1 0 0 0-1 1v12a1 1 0 0 0 1 1h8a1 1 0 0 0 1-1V4.5h-2z" />
                       </svg>
                       <p style="color: #337ab7;">(Presentation - @file.CreatedOn.ToString("dd MMMM yyyy") - @(file.UserFirstName + ' ' + file.UserLastName))</p>
                       <p style="color: #234465;">@file.LectureTitle</p>
                   </div>
               </a>
           </div>
        }
    </div>

    <div class="row mb-3">
        <p class="text-center">
            <a asp-action="MarkAsCompleted" asp-route-id="@this.Model.Id" asp-route-courseId="@this.Model.LectureCourseId" class="btn btn-success w-50">MARK AS COMPLETE</a>
        </p>
    </div>

