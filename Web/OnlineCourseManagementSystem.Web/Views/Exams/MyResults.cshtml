<body class="text-dark">
    <h2>
        <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-bar-chart-line-fill" viewBox="0 0 16 16">
            <path d="M11 2a1 1 0 0 1 1-1h2a1 1 0 0 1 1 1v12h.5a.5.5 0 0 1 0 1H.5a.5.5 0 0 1 0-1H1v-3a1 1 0 0 1 1-1h2a1 1 0 0 1 1 1v3h1V7a1 1 0 0 1 1-1h2a1 1 0 0 1 1 1v7h1V2z" />
        </svg>
        @this.ViewData["Title"]
    </h2>
    <hr />

    <button id="pdf" class="btn btn-outline-primary mb-3">PDF</button>
    <a id="excel" asp-action="ExportMyResultsAsExcel" class="btn btn-outline-success mb-3">Excel</a>
    <a id="csv" asp-action="ExportMyResultsAsCSV" class="btn btn-outline-danger mb-3">CSV</a>

    <form class="float-end" style="display: inline;">
        <div class="row">
            <div class="col-8">
                <input asp-for="Input" class="form-control" />
            </div>
            <div class="col-md-4">
                <button type="submit" class="btn btn-success">Search</button>
            </div>
        </div>
    </form>

    @if (this.Model.Exams.Count() == 0)
    {
        <h1 class="text-center">No Results</h1>
    }
    else
    {

        <div class="row">
            <div class="col-md-12">
                <table id="myTable" class="table table-bordered table-striped table-hover">
                    <thead>
                        <tr>
                            <td class="priority-1">Name</td>
                            <td class="priority-2">Result</td>
                            <td class="priority-3">Points</td>
                            <td class="priority-4">Correct answers</td>
                            <td class="priority-5">Course</td>
                            <td class="priority-6">Active from</td>
                            <td class="priority-7">Time</td>
                            <td class="priority-8">Status</td>
                            <td class="priority-9">Active to</td>
                            <td class="priority-10">Seen on</td>
                        </tr>
                    </thead>
                    <tbody>
                        @foreach (var result in this.Model.Exams)
                        {
                            <tr>
                                <td class="priority-1">
                                    <a asp-controller="Exams" asp-action="Review" asp-route-id="@result.ExamId">@result.ExamName (View result)</a>
                                </td>
                                <td class="priority-2">
                                    @if (result.SuccessRate < 50.00)
                                    {
                                        <span class="badge bg-danger">@result.SuccessRate.ToString("f2")%</span>
                                    }
                                    else if (result.SuccessRate >= 50.00 && result.SuccessRate < 75.00)
                                    {
                                        <span class="badge bg-warning">@result.SuccessRate.ToString("f2")%</span>
                                    }
                                    else if (result.SuccessRate >= 75.00 && result.SuccessRate < 90.00)
                                    {
                                        <span class="badge bg-primary">@result.SuccessRate.ToString("f2")%</span>
                                    }
                                    else
                                    {
                                        <span class="badge bg-success">@result.SuccessRate.ToString("f2")%</span>
                                    }
                                </td>
                                <td class="priority-3">@result.PointsEarned / @result.TotalPoints</td>
                                <td class="priority-4">@result.CorrectAnswers / @result.TotalAnswers</td>
                                <td class="priority-5">@result.ExamCourseName</td>
                                <td class="priority-6">@result.ExamStartDate</td>
                                <td class="priority-7">@result.TimeSpent / @result.ExamDuration</td>
                                <td class="priority-8">
                                    @if (result.Status == OnlineCourseManagementSystem.Data.Models.Enumerations.Status.Graded)
                                    {
                                        <span class="badge bg-primary">@result.Status</span>
                                    }
                                    else if (result.Status == OnlineCourseManagementSystem.Data.Models.Enumerations.Status.Pending)
                                    {
                                        <span class="badge bg-warning">@result.Status</span>
                                    }
                                    else
                                    {
                                        <span>No info</span>
                                    }
                                </td>
                                <td class="priority-9">@result.ExamEndDate</td>
                                <td class="priority-10">
                                    @if (result.SeenOn == null)
                                    {
                                        <p>Not seen yet</p>
                                    }
                                    else
                                    {
                                        <p>@result.SeenOn</p>
                                    }
                                </td>
                            </tr>
                        }
                    </tbody>
                </table>
                <partial name="_MyResultsPagingPartial" model="@this.Model" />
            </div>
        </div>
    }

    <script src="https://cdnjs.cloudflare.com/ajax/libs/html2pdf.js/0.10.1/html2pdf.bundle.min.js" integrity="sha512-GsLlZN/3F2ErC5ifS5QtgpiJtWd43JWSuIgh7mbzZ8zBps+dvLusV+eNQATqgA/HdeKFVgA5v3S/cIrLF7QnIg==" crossorigin="anonymous" referrerpolicy="no-referrer"></script>

    <script>
        $(document).ready(function () {
            $('#myTable').DataTable({
                dom: 'Blfrtip',
                buttons: [
                    'copy', 'csv', 'excel', 'pdf', 'print'
                ]
            });
        });
    </script>
</body>

@section Scripts {
    <script type="text/javascript">
        $("#pdf").click(function () {
            var element = document.getElementById("myTable");

            html2pdf().from(element).set({
                margin: [30, 10, 5, 10],
                pagebreak: { avoid: 'tr' },
                jsPDF: { orientation: 'landscape', unit: 'pt', format: 'letter', compressPDF: false }
            }).save()
        });
    </script>
}